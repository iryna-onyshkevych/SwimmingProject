@model DTO.Models.SwimStyleDTO
@{
    ViewBag.Title = "Deleting SwimStyle";
}

<div>
    <dl class="dl-horizontal">
        <dt>Style Name</dt>
        <dd>
            @Html.DisplayFor(model => model.StyleName)
        </dd>

        
    </dl>

    <div>
        <form asp-controller="SwimStyle" asp-action="Delete" method="post">
            <div class="form-group">
                <input type="submit" class="btn btn-default" value="Delete" />
            </div>
        </form>
    </div>
</div>