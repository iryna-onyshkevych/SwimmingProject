@model IEnumerable<DTO.Models.SwimmerDTO>
@{
    ViewBag.Title = "All swimmers";
}
<a asp-action="create">Add swimmer</a>
<a asp-action="Delete">Delete swimmer</a>
@*<a asp-action="Update">Update coach</a>*@
<table class="table">
    <tr><td>Name</td><td>Surname</td><td>Age</td></tr>
    @foreach (var item in Model)
    {
        <tr><td>@item.FirstName</td><td>@item.LastName</td><td>@item.Age</td></tr>
    }
</table>