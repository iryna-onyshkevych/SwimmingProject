@model DTO.Models.TrainingDTO
@{
    ViewBag.Title = "Add Training";
}
<form asp-action="create" asp-controller="Coaches">
    <div class="form-group">
        <label asp-for="SwimmerId" class="control-label">SwimmerId</label>
        <input type="text" asp-for="SwimmerId" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="SwimStyleId" class="control-label">Surname</label>
        <input type="text" asp-for="SwimStyleId" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="TrainingDate" class="control-label">Date</label>
        <input type="text" asp-for="TrainingDate" class="hasDatepicker" />
    </div>
    <div class="form-group">
        <label asp-for="Distance" class="control-label">Distance</label>
        <input type="number" asp-for="Distance" class="form-control" />
    </div>
    <div class="form-group">
        <input type="submit" value="Submit" class="btn btn-default" />
    </div>
</form>