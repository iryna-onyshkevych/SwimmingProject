@model IEnumerable<DTO.Models.TrainingDTO>
@{
    ViewBag.Title = "All trainings";
}
<a asp-action="create">Add training</a>
@*<a asp-action="Delete">Delete swimmer</a>*@
@*<a asp-action="Update">Update coach</a>*@
<table class="table">
    <tr><td>SwimmerId</td><td>Date</td><td>Distance</td></tr>
    @foreach (var item in Model)
    {
        <tr><td>@item.SwimmerId</td><td>@item.TrainingDate</td><td>@item.Distance</td></tr>
    }
</table>